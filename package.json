{
  "name": "liebherr-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:core",
    "start:core": "ng serve core  -o --port 2000",
    "start:dashboard": "ng serve dashboard -o --port 3000",
    "start:sidenav": "ng serve sidenav -o --port 4000",
    "start:all": "concurrently \"npm run start:core\" \"npm run start:dashboard\" \"npm run start:sidenav\" ",
    "build:core": "ng build core --prod",
    "build:dashboard": "ng build dashboard --prod",
    "build:sidenav": "ng build sidenav --prod",
    "serve:dist": "concurrently \"serve dist/core -l 2000 -s\" \"serve dist/dashboard -l 3000 -s\" \"serve dist/sidenav -l 4000 -s\" ",
    "build": "npm run build:core && npm run build:dashboard && npm run build:sidenav",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.0.2",
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "ngx-easy-table": "^15.1.0",
    "rxjs": "~7.4.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.5.2"
  }
}
